Visualization Manager:
  Class: ""
  Displays:
    - Class: rviz_default_plugins/RobotModel
      Enabled: true
      Name: RobotModel
      Robot Description: robot_description

    - Class: rviz_default_plugins/LaserScan
      Enabled: true
      Name: LidarScan
      Topic: /scan
      Queue Size: 10
      Size (m): 0.05
      Color: 0; 0; 1
      Style: Flat Squares
      Decay Time: 0

  Global Options:
    Fixed Frame: base_link
